    "compilerOptions": {
      "target":"es6"
      , "module": "commonjs"
      ,"sourceMap": true
      ,"noEmitOnError": true
      ,"noUnusedParameters":true
      ,"moduleResolution": "node"
      ,"allowSyntheticDefaultImports":true
      ,"jsx":"preserve"
      ,"lib":["dom"
           ,"es2015.core"
           ,"es2015.collection"
           ,"es2015.iterable"
           ,"es2015.proxy"
           ,"es2015.reflect"
           ,"es2015.generator"
           ,"es2015.symbol"
           ,"es2015.symbol.wellknown"
